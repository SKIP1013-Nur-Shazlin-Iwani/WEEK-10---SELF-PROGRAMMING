package myArray;

import java.util.Arrays;

class FoundIndex {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] num = {10,20,25,5,35};
		
		Arrays.sort(num);
		
		int key = 25;
		
		System.out.println(key + " found at index = " + Arrays.binarySearch(num, key));

	}

}
